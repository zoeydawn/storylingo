{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "pluginSearchDeps": true,
  "overrides": [
    {
      "files": ["*.svelte", "*.ts"],
      "options": {
        "parser": "svelte",
        "trailingComma": "all"
      }
    },
    {
      "files": "package.json",
      "options": {
        "printWidth": 0
      }
    }
  ]
}
